{
    "mingw":[
        {
            "Prompt":"g++",
            "Prompt Description":"c/c++ compiler",
            "Prompt Example":"g++ <cpp_file>",
            "Prompt Parameters":[],
            "Prompt Flags":[
                {
                    "Flag": "-I",
                    "Flag Description": "mingw: include src files",
                    "Flag Example":"g++ <cpp_file> -L<header_file> -I<src_files_directory -o<output_executable>"
                },
                {
                    "Flag": "-L",
                    "Flag Description": "mingw: include libraries",
                    "Flag Example":"g++ <cpp_file> -L<header_file> -I<src_files_directory -o<output_executable>"
                },
                {
                    "Flag": "-o",
                    "Flag Description": "mingw: output to executable file",
                    "Flag Example":"g++ <cpp_file> -L<header_file> -I<src_files_directory -o<output_executable>"
                }
            ]
        },
        {   
            "Prompt":"mingw-make",
            "Propmt Description":"mingw-make: read compile make file for mingw compiler",
            "Propmt Example":"mingw-make <cpp_file>",
            
            "Prompt Parameters":[],
            "Prompt Flags":[
                {
                    "Flag": "--version",
                    "Flag Description": "mingw-make: list version details",
                    "Flag Example":"g++ <cpp_file> -L<header_file> -I<src_files_directory -o<output_executable>"
                }
            ]
        }
    ]
}